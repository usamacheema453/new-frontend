      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a0fdfcf2-a070-4bea-a647-26846951f8b7"
      }
    },
    "plugins": [
      "expo-audio",
      "expo-video"
    ]
